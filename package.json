{
  "name": "@jiangshengdev/mini-vue",
  "private": false,
  "version": "0.0.1-beta.1",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/jiangshengdev/mini-vue#readme",
  "bugs": {
    "url": "https://github.com/jiangshengdev/mini-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangshengdev/mini-vue.git"
  },
  "author": "Jiang Sheng <jiangshengdev@outlook.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "vite",
    "play:build": "tsc && vite build",
    "play:preview": "vite preview",
    "test": "vitest run",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "xo": "xo",
    "format": "prettier docs playground src test --write",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.30.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "oxc-minify": "^0.99.0",
    "oxlint": "~1.30.0",
    "prettier": "3.6.2",
    "tsdown": "^0.16.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.2",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^3.2.4",
    "xo": "^1.2.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.2"
    },
    "onlyBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
