{
  "name": "@jiangshengdev/mini-vue",
  "private": false,
  "version": "0.0.1-beta.32",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/jiangshengdev/mini-vue#readme",
  "bugs": {
    "url": "https://github.com/jiangshengdev/mini-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangshengdev/mini-vue.git"
  },
  "author": "Jiang Sheng <jiangshengdev@outlook.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./jsx": "./dist/jsx/index.d.ts",
    "./jsx-runtime": "./dist/index.mjs",
    "./jsx-dev-runtime": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  },
  "scripts": {
    "clean": "rm -rf dist etc temp docs/.vitepress/cache docs/.vitepress/dist .eslintcache",
    "build": "tsdown && pnpm run generate:jsx-shim",
    "generate:jsx-shim": "node scripts/generate-jsx-shim.ts",
    "check:cross-boundary-import-style": "node scripts/check/import/check-cross-boundary-import-style.ts",
    "check:duplicate-import-export": "node scripts/check/import/check-duplicate-import-export.ts",
    "check:import-export-sorted": "node scripts/check/import/check-import-export-sorted.ts",
    "check:import-export-type-mixed": "node scripts/check/import/check-import-export-type-mixed.ts",
    "check:index-export-local-only": "node scripts/check/import/check-index-export-local-only.ts",
    "check:props-destructure": "node scripts/check/props-destructure.ts",
    "check:src-import-boundary": "node scripts/check/import/check-src-import-boundary.ts",
    "check:test-prefer-root-index": "node scripts/check/import/check-test-prefer-root-index.ts",
    "check:test-src-import-style": "node scripts/check/import/check-test-src-import-style.ts",
    "check:import": "node scripts/check/import/run.ts",
    "check": "node --import tsx scripts/check/run.ts",
    "dev": "tsdown --watch",
    "play": "vite",
    "play:build": "tsc && vite build",
    "play:preview": "vite preview",
    "test": "vitest run",
    "test:browser": "vitest run --config=vitest.browser.config.ts",
    "test:inspect": "pnpm run test:browser --inspect-brk --no-file-parallelism",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:all": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:headed": "playwright test --project=chromium --headed",
    "test:e2e:all:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --project=chromium --ui",
    "test:e2e:list": "playwright test --project=chromium --list",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "xo": "xo",
    "preflight": "pnpm run fmt && pnpm run typecheck && pnpm run lint && pnpm run xo",
    "ci": "pnpm run preflight && pnpm run check",
    "format": "prettier .github .kiro docs e2e playground scripts src test --write",
    "fmt": "prettier .github .kiro docs e2e playground scripts src test --write --log-level silent",
    "typecheck": "tsc --noEmit",
    "api": "mkdir -p etc && api-extractor run --local --verbose -c api-extractor.jsonc",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@fast-check/vitest": "^0.2.4",
    "@playwright/test": "^1.57.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@vitest/browser": "^4.0.15",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.30.0",
    "fast-check": "^4.5.2",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "markdown-it-mathjax3": "^4.3.2",
    "npm-run-all2": "^8.0.4",
    "oxc-minify": "^0.99.0",
    "oxlint": "~1.30.0",
    "playwright": "^1.57.0",
    "prettier": "3.6.2",
    "tsdown": "^0.16.4",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.2",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^4.0.15",
    "xo": "^1.2.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.2"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
